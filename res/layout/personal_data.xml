<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:background="@color/list_background"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/ebpay_text_orange1">
    <ImageView

        android:layout_centerVertical="true"
        android:id="@+id/iconfont_xinxi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tb_icon_more_msg_white_56"
        android:layout_alignParentRight="true"/>
    <TextView
        android:id="@+id/tv_sttings"
        android:layout_marginRight="50dp"
        android:layout_toLeftOf="@+id/iconfont_xinxi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="设置"
        android:textColor="#fff"
        android:textSize="20sp"
        android:layout_centerInParent="true"/>
        </RelativeLayout>
    <RelativeLayout
        android:background="@color/ebpay_text_orange1"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_centerVertical="true"
            android:id="@+id/default_avatar"
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/default_avatar"/>
        <TextView
            android:id="@+id/tv_username"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="周国庆"
            android:textSize="20sp"
            android:textColor="#fff"
            android:layout_toRightOf="@+id/default_avatar"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/scan"
            android:layout_toRightOf="@+id/tv_username"
            android:layout_marginLeft="20dp"/>
    </RelativeLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#DF4600">
            <RelativeLayout
                android:id="@+id/rl_soucang_baobei"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">
                <TextView
                    android:layout_centerHorizontal="true"
                    android:id="@+id/shoucangNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:textSize="20sp"
                    android:textColor="#fff" />
                <TextView
                    android:layout_centerHorizontal="true"

                    android:layout_below="@+id/shoucangNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="收藏的宝贝"
                    android:textSize="20sp"
                    android:textColor="#fff"/>
                <View
                    android:layout_centerInParent="true"
                    android:layout_width="1dp"
                    android:layout_height="30dp"
                    android:background="@color/white"
                    android:layout_alignParentRight="true">

                </View>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_soucang_dianpu"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">
                <TextView
                    android:layout_centerHorizontal="true"
                    android:id="@+id/dianpuNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="28"
                    android:textSize="20sp"
                    android:textColor="#fff" />
                <TextView
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/dianpuNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="收藏的店铺"
                    android:textSize="20sp"
                    android:textColor="#fff"/>
                <View
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp"
                    android:layout_height="30dp"
                    android:background="@color/white"
                    android:layout_centerInParent="true">
                </View>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_zhuji"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">
                <TextView
                    android:layout_centerHorizontal="true"
                    android:id="@+id/zujiNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="58"
                    android:textSize="20sp"
                    android:textColor="#fff" />
                <TextView
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/zujiNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的足迹"
                    android:textSize="20sp"
                    android:textColor="#fff"/>
                <View
                    android:layout_alignParentRight="true"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:layout_centerInParent="true">
                </View>
            </RelativeLayout>
        </LinearLayout>
    <RelativeLayout
        android:id="@+id/My_dingdan"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的订单"
            android:textColor="#000"
            android:textSize="20sp"/>
        <TextView
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/im_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="查看全部已购买宝贝"
            android:textColor="#000"
            android:textSize="15sp"
            android:layout_marginRight="10dp"
            />
        <ImageView
            android:layout_marginRight="20dp"
            android:layout_centerVertical="true"
            android:id="@+id/im_arrow"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:src="@drawable/wallet_base_indicator_arrow"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <LinearLayout
        android:layout_marginTop="2dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/order_list1"/>
        <ImageView
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/order_list2"/>
        <ImageView
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/order_list3"/>
        <ImageView
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/order_list4"/>
        <ImageView
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/order_list5"/>
    </LinearLayout>
    <RelativeLayout
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="30dp"
            android:textSize="20dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的卡券包"
            android:textColor="#000"/>
        <ImageView
            android:layout_marginRight="20dp"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/wallet_base_indicator_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="3dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:id="@+id/shenhuajuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/center_card_coupon1"/>
        <TextView
            android:layout_toRightOf="@+id/shenhuajuan"
            android:layout_marginLeft="20dp"
            android:textSize="20dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="生活卷"
            android:textColor="#000"/>
        <ImageView
            android:layout_marginRight="20dp"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/wallet_base_indicator_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="3dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:id="@+id/wangdianjuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/center_card_coupon2"/>
        <TextView
            android:layout_toRightOf="@+id/wangdianjuan"
            android:layout_marginLeft="20dp"
            android:textSize="20dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="网店卷"
            android:textColor="#000"/>
        <TextView
            android:layout_marginRight="20dp"
            android:layout_toLeftOf="@+id/wangdianim"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无可用优惠券"
            android:textSize="15dp"
            android:textColor="#000"
            />
        <ImageView
            android:id="@+id/wangdianim"
            android:layout_marginRight="20dp"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/wallet_base_indicator_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="3dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:id="@+id/huiyuanka"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/center_card_coupon3"/>
        <TextView
            android:layout_toRightOf="@+id/huiyuanka"
            android:layout_marginLeft="20dp"
            android:textSize="20dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="会员卡"
            android:textColor="#000"/>
        <TextView
            android:layout_marginRight="20dp"
            android:layout_toLeftOf="@+id/huiyuankaim"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="一卡在手，逛店无忧"
            android:textSize="15dp"
            android:textColor="#000"
            />
        <ImageView
            android:id="@+id/huiyuankaim"
            android:layout_marginRight="20dp"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/wallet_base_indicator_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="13dp"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:id="@+id/Mytequan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <TextView
            android:layout_toRightOf="@+id/Mytequan"
            android:layout_marginLeft="20dp"
            android:textSize="20dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的专享特权"
            android:textColor="#000"/>
        <TextView
            android:layout_marginRight="20dp"
            android:layout_toLeftOf="@+id/MytequanImage"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="一卡在手，逛店无忧"
            android:textSize="15dp"
            android:textColor="#000"
            />
        <ImageView
            android:id="@+id/MytequanImage"
            android:layout_marginRight="20dp"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/wallet_base_indicator_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
</LinearLayout>